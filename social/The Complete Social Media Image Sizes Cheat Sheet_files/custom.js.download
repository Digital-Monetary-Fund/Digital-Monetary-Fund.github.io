jQuery(window).scroll(function() {    
  var scroll = jQuery(window).scrollTop();

  if (scroll >= 150) {
      jQuery(".hd-grey-bg").addClass("header-sticky");
  } else {
      jQuery(".hd-grey-bg").removeClass("header-sticky");
  }
});


