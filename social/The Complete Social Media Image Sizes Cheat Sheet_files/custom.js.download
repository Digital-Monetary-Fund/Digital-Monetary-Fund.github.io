jQuery(document).ready(function () {
  jQuery('input.price-switch[type="checkbox"]').click(function () {
    if (jQuery(this).prop("checked") == true) {
      jQuery(this).val("true");
      jQuery(".yearly_span").removeClass("active");
      jQuery(".yearly_plan").removeClass("hide");
      jQuery(".monthly_plan").addClass("hide");
    } else if (jQuery(this).prop("checked") == false) {
      jQuery(this).val("false");

      jQuery(".yearly_span").addClass("active");
      jQuery(".yearly_plan").addClass("hide");
      jQuery(".monthly_plan").removeClass("hide");
    }

    try {
      dataLayer.push({
        event: "pricing_plan_toggle",
        eventCategory: "Pricing Plan Toggle",
        eventAction: jQuery(this).prop("checked") ? "Yearly" : "Monthly",
      });
    } catch (e) {
      console.log(e);
    }
  });
});

