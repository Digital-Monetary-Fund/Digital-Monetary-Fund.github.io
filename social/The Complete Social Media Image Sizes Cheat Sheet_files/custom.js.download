jQuery(document).ready(function () {
  $t = jQuery('[data-event="ev"]');
  $t.click(function () {
    var evCat = jQuery(this).data("category")
        ? jQuery(this).data("category")
        : "",
      evAct = jQuery(this).data("action") ? jQuery(this).data("action") : "",
      evLab = jQuery(this).data("label") ? jQuery(this).data("label") : "",
      evName = jQuery(this).data("evname")
        ? jQuery(this).data("evname")
        : "ButtonClick";
    try {
      dataLayer.push({
        event: evName,
        eventCategory: evCat,
        eventAction: evAct,
        eventLabel: evLab,
      });
    } catch (e) {
      console.log(e);
    }
  });

  jQuery(".om-form-btn").click(function () {
    var btn = jQuery(this);
    var id = btn.attr("id");

    showDealName = id;
  });
});
